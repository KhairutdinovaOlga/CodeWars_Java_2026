public class FakeBinary {
    public static String fakeBin(String numberString) {
       String result="";
      for (int i=0; i<numberString.length();i++){
        char d=numberString.charAt(i);
        if(d<'5'){
          result=result+"0";
        }else {
          result = result+"1";
        }
      }
      return result;
    }
}
